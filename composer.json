{
    "name": "thomasdominic/eloquent-model-testor",
    "description": "Test easier your Eloquent Models in your Laravel Project",
    "keywords": [
        "thomasdominic",
        "eloquent-model-testor",
        "Eloquent",
        "relation",
        "laravel"
    ],
    "homepage": "https://github.com/thomasdominic/eloquent-model-testor",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dominic THOMAS",
            "email": "dthomas@codenco.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Thomasdominic\\ModelsTestor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thomasdominic\\ModelsTestor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
